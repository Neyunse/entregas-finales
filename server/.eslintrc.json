{
    "extends": ["prettier", "eslint:recommended"],
    "rules": {
        "semi": "off",
        "quotes": "off",
        "no-unused-vars": "warn",
        "no-undef": "off",
        "no-case-declarations": "off"
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    }
}
